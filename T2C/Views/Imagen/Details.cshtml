@model T2C.Models.ImagenModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Administrator.cshtml";
}

<h2>Detalle</h2>

<div class="col-md-10">
    <div class="col-md-2">
        <div class="property-images imagen-center ">
            <img src=@Model.PathImagen width="100" height="100" />
        </div>
    </div>
    <div class="col-md-2 property-info">
        <div class="row">
            <strong>@Html.DisplayNameFor(model => model.NameType)</strong>
            @Html.DisplayFor(model => model.NameType)
        </div>
        <div class="row">
            <strong>
                @Html.DisplayNameFor(model => model.Type)</strong>
            @Html.DisplayFor(model => model.Type)
        </div>

    </div>


</div>
<div class="col-md-3">
    <p>
        @Html.ActionLink("Editar", "Edit", new { id = Model.Id }) |
        @Html.ActionLink("Volver", "Index")
    </p>
</div>



